!! ~/.Xresources

!!
!! rxvt-unicode (urxvt)
!!

URxvt.geometry:             96x32
URxvt.scrollBar_right:      true
URxvt.scrollBar_floating:   true
URxvt.scrollstyle:          plain
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      true
URxvt.pointerBlank:         true
URxvt.visualBell:           false
URxvt.saveLines:            3000
URxvt.fading:               40

!! Font list and Spacing
URxvt.font:                 xft:Ricty
URxvt.letterSpace:          -1
!URxvt.lineSpace:            0

!! color scheme https://github.com/xero/sourcerer/blob/master/sourcerer.Xresources
URxvt.depth:                32

URxvt.foreground:   [90]#d3d3d3
URxvt.background:   [90]#111111
! URxvt.cursorColor:  [90]#c2c2b0

! black
*.color0:       [90]#111111
*.color8:       [90]#181818

! red
*.color1:       [90]#aa4450
*.color9:       [90]#ff6a6a

! green
*.color2:       [90]#719611
*.color10:      [90]#b1d631

! yellow
*.color3:       [90]#ff9800
*.color11:      [90]#87875f

! blue
*.color4:       [90]#6688aa
*.color12:      [90]#90b0d1

! magenta
*.color5:       [90]#8f6f8f
*.color13:      [90]#8181a6

! cyan
*.color6:       [90]#528b8b
*.color14:      [90]#87ceeb

! white
*.color7:       [90]#d3d3d3
*.color15:      [90]#c1cdc1

! stylized
*.colorIT:      [90]#719611
*.colorBD:      [90]#d3d3d3
*.colorUL:      [90]#528b8b

!Xterm escapes, word by word movement
URxvt.keysym.Control-Left:    \033[1;5D
URxvt.keysym.Control-Right:    \033[1;5C
